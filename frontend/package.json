{
  "name": "dltmanager-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "ci:build:prd": "ng build --configuration production",
    "ci:build:abn": "ng build --configuration abnahme",
    "ci:build:tst": "ng build --configuration intern",
    "ci:audit": "npm audit --registry=https://registry.npmjs.org --audit-level=high --omit=dev",
    "ci:test": "ng test --coverage",
    "ci:e2e": "cypress run --component",
    "ci:lint": "ng lint",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --coverage --watch",
    "lint": "ng lint",
    "e2e": "cypress open --component -b electron",
    "audit-fix": "npm audit fix --registry=https://registry.npmjs.org --audit-level=high --omit=dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/cdk": "^19.2.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "angular-oauth2-oidc": "19.0.0",
    "ngx-logger": "^5.0.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^19.0.0",
    "@angular-devkit/build-angular": "^19.2.0",
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/cli": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@eslint/js": "^9.27.0",
    "@faker-js/faker": "^9.8.0",
    "@signal-iduna/ui": "^3.68.2",
    "@signal-iduna/ui-angular-proxy": "^3.68.2",
    "@types/jest": "^29.5.14",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "angular-eslint": "^19.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "cypress": "^14.4.0",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.5.0",
    "prettier": "^3.3.3",
    "sinon": "^20.0.0",
    "ts-jest": "^29.3.4",
    "typescript": "~5.6.0",
    "typescript-eslint": "^8.32.1"
  }
}
